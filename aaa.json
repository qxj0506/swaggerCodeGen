						}
					}
				],
				"requestBody": {
					"content": {
						"application/json": {
							"schema": {
								"$ref": "#/components/schemas/SdmSubscription"
							}
						}
					},
					"required": true
				},
				"responses": {
					"201": {
						"description": "Expected response to a valid request",
						"content": {
							"application/json": {
								"schema": {
									"$ref": "#/components/schemas/SdmSubscription"
								}
							}
						}
					},
					"404": {
